{
  "expo": {
    "name": "Renvo",
    "slug": "renvo-mobile",
    "version": "1.0.0",
    "orientation": "portrait",

    "userInterfaceStyle": "light",
          "splash": {
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.renvo.mobile"
    },
            "android": {
          "package": "com.renvo.mobile"
        },
        "web": {
        },
    "plugins": [
      "expo-apple-authentication",
      "expo-camera",
      "expo-image-picker",
      "expo-media-library"
    ]
  }
} 